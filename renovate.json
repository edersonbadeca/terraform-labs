{
  "extends": [
    "config:base"
  ],
  "ignoreTests": true,
  "packageRules": [
    {
      "matchDatasources": [
        "terraform-module"
      ],
      "automerge": true,
      "automergeType": "branch"
    }
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "recreateClosed": true,
    "rebaseStalePrs": true
  }
}
